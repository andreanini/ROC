export(det, det.sre, det.matrix,        # create DET statistics
       read.sre, read.tnt,              # read SRE, collection of supervised trials structure
       as.cst, cst.tnt,                    # bless structure to be class "cst"
       roc, roc.cst, plot.roc,
       logit, sigmoid,                  # helper functions for probabilities
       eer, Cllr.tnt,                   # ROC convex hull implementation
       plot.det, summary.det,
       plot.det.cst,
       plot.cond,
       legend.det,                      # make a legend for a DET plot
       ape.plot,                         # APE plot
       ece.tnt, ece.plot,
       bayes.error.rate, minDCF, actDCF,
       nbe.plot,                        # Normalized Bayes Error rate plot
       cond.table,                      # compute condition contingency table
       setDCF, prior.log.odds, DCF,      # set DCF parameters
       tippet.plot, roc.plot,
       ddensity, plot.ddensity
       )

